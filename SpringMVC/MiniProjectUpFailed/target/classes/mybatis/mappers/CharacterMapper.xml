<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 

<mapper namespace="com.bit.juniver.model.dao.CharacterMapper">
	<select id='listCharacter' parameterType='int' resultType="com.bit.juniver.model.dto.CharacterBean">
		select * from character where menu_id = #{menuID}
	</select>

	<select id='getCharacter' parameterType='int' resultType="com.bit.juniver.model.dto.CharacterBean">
		select * from character where id = #{id}
	</select>
</mapper>